{
  "name": "shadcn-table",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf --glob **/node_modules **/dist **/.next pnpm-lock.yaml **/.tsbuildinfo",
    "build": "next build",
    "dev": "next dev",
    "dev:docker": "pnpm db:start && pnpm dev",
    "start": "next start",
    "ollie": "pnpm install && pnpm run db:start && pnpm run db:setup",
    "lint": "biome check .",
    "lint:fix": "biome check . --write",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck",
    "shadcn": "pnpm dlx shadcn@latest",
    "build:registry": "shadcn build",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "db:generate": "dotenv drizzle-kit generate",
    "db:introspect": "dotenv drizzle-kit introspect",
    "db:push": "dotenv drizzle-kit push",
    "db:migrate": "dotenv tsx src/db/migrate.ts",
    "db:drop-migration": "drizzle-kit drop",
    "db:seed": "dotenv tsx src/db/seed.ts",
    "db:setup": "pnpm db:push && pnpm db:seed",
    "db:reset": "pnpm db:stop && rimraf docker-data && pnpm db:start && sleep 10 && pnpm db:setup",
    "db:studio": "dotenv drizzle-kit studio"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.1.1",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-portal": "^1.1.9",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.2",
    "export-to-csv": "^1.4.0",
    "geist": "^1.4.2",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.0",
    "nanoid": "^5.1.5",
    "next": "^15.3.4",
    "next-themes": "^0.4.6",
    "nuqs": "^2.4.3",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-day-picker": "9.7.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "server-only": "^0.0.1",
    "shadcn": "2.7.0",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "zod": "^3.25.71"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@faker-js/faker": "^9.9.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.4",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.23.1"
  },
  "packageManager": "pnpm@10.9.0"
}

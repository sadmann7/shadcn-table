    "geist": "^1.2.2",
    "million": "^3.0.4",
    "nanoid": "^5.0.6",
    "next": "14.2.0-canary.12",
    "next-themes": "^0.2.1",
    "postgres": "^3.4.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
